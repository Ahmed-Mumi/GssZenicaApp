@model GssZenicaApp.ViewModels.AddMembershipFeeCategoryViewModel
@{
    Layout = null;
}
<form asp-action="Create" class="form-horizontal" asp-controller="MembershipFeeCategory" onsubmit="return jQueryAjaxPost(this);">
    <div class="form-group">
        <label>Naziv tipa članarine: </label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label>Iznos članarine</label>
        <input asp-for="Amount" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" class="btn btn-success" value="Snimi" />
    </div>
</form>